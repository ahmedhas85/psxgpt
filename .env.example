# API Keys for psxGPT
# Replace the placeholder values below with your actual API keys

# Google Gemini API Key (get from https://ai.google.dev/)
GEMINI_API_KEY=your_gemini_api_key_here

# Anthropic API Key for Claude (get from https://www.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LlamaParse API Key for PDF processing (get from https://docs.cloud.llamaindex.ai/llamacloud/getting_started/api_key)
LLAMA_CLOUD_API_KEY=your_llama_cloud_api_key_here

# Mistral API Key for OCR (optional, get from https://console.mistral.ai/)
MISTRAL_API_KEY=your_mistral_api_key_here

# Database URLs (replace 'your_password' with your PostgreSQL password)
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/analyst_psx
DATABASE_DIRECT_URL=postgresql://postgres:your_password@localhost:5432/analyst_psx
